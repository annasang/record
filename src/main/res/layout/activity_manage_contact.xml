<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".ManageContact">

    <Button
        android:id="@+id/open"
        android:layout_width="112dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:alpha="0.75"
        android:background="@android:drawable/alert_light_frame"
        android:text="OPEN LIST"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/updatecon"
        android:layout_width="206dp"
        android:layout_height="63dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:alpha="0.75"
        android:background="@android:drawable/alert_light_frame"
        android:gravity="center_vertical|center_horizontal"
        android:padding="2dp"
        android:text="Update Data"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ringer" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="71dp" />

    <EditText
        android:id="@+id/gps"
        android:layout_width="227dp"
        android:layout_height="53dp"
        android:layout_marginTop="16dp"
        android:alpha="0.5"
        android:ems="10"
        android:hint="New GPS Code"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/selDel"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/ringer"
        android:layout_width="229dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:alpha="0.5"
        android:ems="10"
        android:hint="New Ringer Code"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/gps"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/selDel"
        android:layout_width="227dp"
        android:layout_height="58dp"
        android:layout_marginTop="16dp"
        android:alpha="0.5"
        android:ems="10"
        android:hint="ID"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="12sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

</android.support.constraint.ConstraintLayout>